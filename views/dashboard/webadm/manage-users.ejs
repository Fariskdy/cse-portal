                                    <option value="ADMINISTRATOR" <%=user.role==='ADMINISTRATOR' ? 'selected' : '' %>>Administrator</option>
                                    <option value="ACCOUNTANT" <%=user.role==='ACCOUNTANT' ? 'selected' : '' %>>Accountant</option>
                                </select>
                                <input type="submit" value="update" class="btn">
                            </form></td>
