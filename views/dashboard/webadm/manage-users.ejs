                                    <option value="MODERATOR" <%=user.role==='MODERATOR' ? 'selected' : '' %>>Moderator
                                    </option>
                                    <option value="REGION_MANAGER" <%=user.role==='REGION_MANAGER' ? 'selected' : '' %>>Region Manager</option>
